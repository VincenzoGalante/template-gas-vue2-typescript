{
  "name": "template-apps-script",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:server": "webpack -c webpack.server.ts",
    "build:client": "vue-cli-service build --no-module --no-clean --mode production",
    "build:staging": "npm run clean && vue-cli-service build --no-module --no-clean --mode staging && cross-env STAGING=true npm run build:server",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf dist",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "create": "mkdir dist && clasp create --type webapp --rootDir dist && mv dist/.clasp.json . && clasp pull && mv dist/appsscript.json . && node scripts/appsscriptHelper.js timezone Europe/Berlin && node scripts/appsscriptHelper.js webapp --access MYSELF --execute USER_DEPLOYING && cp appsscript.json dist/ && clasp push -f && clasp open"
  },
  "dependencies": {
    "core-js": "^3.22.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.1.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.4",
    "vuex": "^3.6.2",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@types/glob": "^7.2.0",
    "@types/google-apps-script": "^1.0.46",
    "@types/jest": "^27.4.1",
    "@types/node": "~16.0.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "gas-webpack-plugin": "^2.2.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "sass": "~1.32.13",
    "sass-loader": "^10.2.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.3",
    "uglify-js": "^3.15.4",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
